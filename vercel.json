{
  "version": 2,
  "public": true,
  "regions": ["fra1"],
  "routes": [
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/favicon-16x16.png", "dest": "/favicon-16x16.png" },
    { "src": "/favicon-32x32.png", "dest": "/favicon-32x32.png" },
    { "src": "/apple-touch-icon.png", "dest": "/apple-touch-icon.png" },
    { "src": "/android-chrome-192x192.png", "dest": "/android-chrome-192x192.png" },
    { "src": "/android-chrome-512x512.png", "dest": "/android-chrome-512x512.png" },
    { "src": "/site.webmanifest", "dest": "/site.webmanifest" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },

    { "source": "/cars", "destination": "/cars.html" },
    { "source": "/car-single", "destination": "/car-single.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/account-dashboard", "destination": "/account-dashboard.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/booking", "destination": "/booking.html" },
    { "source": "/sofer-treaz", "destination": "/sofer-treaz.html" },
    { "source": "/all-codes", "destination": "/all-codes.html" },
    { "source": "/terms", "destination": "/terms.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
