{
  "version": 2,
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/cars",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/cars/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/coupons/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/bookings/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/spinning-wheels/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/cars",
      "dest": "/Rentaly HTML/cars.html"
    },
    {
      "src": "/car-single",
      "dest": "/Rentaly HTML/car-single.html"
    },
    {
      "src": "/login",
      "dest": "/Rentaly HTML/login.html"
    },
    {
      "src": "/account-dashboard",
      "dest": "/Rentaly HTML/account-dashboard.html"
    },
    {
      "src": "/about",
      "dest": "/Rentaly HTML/about.html"
    },
    {
      "src": "/contact",
      "dest": "/Rentaly HTML/contact.html"
    },
    {
      "src": "/booking",
      "dest": "/Rentaly HTML/booking.html"
    },
    {
      "src": "/spinning-wheel-standalone",
      "dest": "/Rentaly HTML/spinning-wheel-standalone.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Rentaly HTML/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
} 