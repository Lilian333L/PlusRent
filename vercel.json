{
  "version": 2,
  "public": true,
  "regions": ["fra1"],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/cars",
      "destination": "/cars.html"
    },
    {
      "source": "/car-single",
      "destination": "/car-single.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/account-dashboard",
      "destination": "/account-dashboard.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/booking",
      "destination": "/booking.html"
    },
    {
      "source": "/sofer-treaz",
      "destination": "/sofer-treaz.html"
    },
    {
      "source": "/all-codes",
      "destination": "/all-codes.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    }
  ],
  "static": {
    "include": [
      "favicon.ico",
      "favicon-16x16.png",
      "favicon-32x32.png",
      "apple-touch-icon.png",
      "android-chrome-192x192.png",
      "android-chrome-512x512.png",
      "site.webmanifest"
    ]
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
