{
  "version": 2,
  "public": true,
  "regions": ["fra1"],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.plusrent.md"
        }
      ],
      "destination": "https://plusrent.md",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.plusrent.md"
        }
      ],
      "destination": "https://plusrent.md/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/spinning-wheel-standalone.html",
      "destination": "/spinning-wheel-standalone.html"
    },
    {
      "source": "/cars",
      "destination": "/cars.html"
    },
    {
      "source": "/car-single",
      "destination": "/car-single.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/account-dashboard",
      "destination": "/account-dashboard.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/booking",
      "destination": "/booking.html"
    },
    {
      "source": "/sofer-treaz",
      "destination": "/sofer-treaz.html"
    },
    {
      "source": "/all-codes",
      "destination": "/all-codes.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
